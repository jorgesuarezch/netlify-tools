{
  "name": "@netlify-tools/cli",
  "description": "manage your environment configuration seamless",
  "version": "0.0.0",
  "author": "Jorge SuÃ¡rez",
  "bin": {
    "netlify-tools": "./bin/run",
    "nfytools": "./bin/run"
  },
  "bugs": "https://github.com/jorgesuarezch/neltify-tools/issues",
  "dependencies": {
    "@netlify-tools/core": "^1",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "chalk": "^4.1.0",
    "lodash": "^4.17.20",
    "netlify": "^4.3.13",
    "tslib": "^2"
  },
  "devDependencies": {
    "@manifoldco/swagger-to-ts": "^2.1.0",
    "@oclif/dev-cli": "^1",
    "@types/lodash": "^4.14.161",
    "eslint": "^7.8",
    "globby": "^11",
    "nock": "^13.0.4",
    "prettier": "^2.1.1",
    "ts-node": "^9",
    "typescript": "^4.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/jorgesuarezch/neltify-tools",
  "keywords": [
    "netlify",
    "environment",
    "cli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "netlify-tools",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jorgesuarezch/neltify-tools.git",
    "directory": "packages/cli"
  },
  "scripts": {
    "lint": "eslint '*/**/*.{js,ts,tsx}' --fix --color --cache --format codeframe",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest --coverage --passWithNoTests",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
